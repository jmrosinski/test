program basic
  use gptl
  use gptl_acc
  
  implicit none

  integer :: n, ret

  ret = gptlstart (0)
!$acc parallel loop private(ret)
  do n=0,0
    ret = gptlstart_gpu (0)
    ret = gptlstop_gpu (0)
  end do
!$acc end parallel
  ret = gptlstop (0)
  ret = gptlcudadevsync ()
  stop 0
end program basic
